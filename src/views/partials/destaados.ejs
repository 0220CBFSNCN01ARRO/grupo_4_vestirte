<div class="container products-wrapper">
    <div class="row">
        <div class="col-12 container">
            <h2 class="products-title border-bottom " >Productos destacados</h2>
        </div>
        <% for (producto of productos) { %>
        <div class="col-12 col-sm-6 col-lg-3">
            <section class="product-box ">
                <!-- <a href="/product/detail/<%= producto.id %>/visitados"> -->
                    <figure class="product-box_image">
                        <img src="/img/<%= producto.image %>" alt="imagen de producto" height="300" width="250" >
                    </figure>
                    <article class="product-box_data">
                        <h2>$<%= producto.precio %></h2>
                       
                       
                    </article>
                <!-- </a> -->
            </section>
        </div>
        <% } %>
    </div>
  </div>
  <!-- /ultima visita -->
  
      
  </div>